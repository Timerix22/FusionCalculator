public static class Calculator {
    public static double Calculate(string exprStr) {
        Lexer() lexer;
        List<Token()> tokens = lexer.Lex(exprStr);
        // lexer.PrintTokens();
        Parser() parser;
        IExpression expr = parser.Parse(tokens);
        // parser.PrintGraphVizCode();
        return expr.Calculate();
    }
}
